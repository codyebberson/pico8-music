{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "ignorePatterns": [
        "dist/**/*.js",
        "examples/*.js",
        "test/**/*.js"
    ],
    "rules": {
        "max-len": ["error", {"code": 120}],
        "no-unused-vars": ["off"]
    }
}
